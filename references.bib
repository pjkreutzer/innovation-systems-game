@article{meyer2018QuantifyingResilienceRecurrent,
  title = {Quantifying Resilience to Recurrent Ecosystem Disturbances Using Flow–Kick Dynamics},
  author = {Meyer, Katherine and Hoyer-Leitzel, Alanna and Iams, Sarah and Klasky, Ian and Lee, Victoria and Ligtenberg, Stephen and Bussmann, Erika and Zeeman, Mary Lou},
  date = {2018-11},
  journaltitle = {Nature Sustainability},
