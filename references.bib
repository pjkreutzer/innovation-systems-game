@article{birch2010SustainableCapitalNeoliberalization,
  title = {Sustainable {{Capital}}? {{The Neoliberalization}} of {{Nature}} and {{Knowledge}} in the {{European}} “{{Knowledge-based Bio-economy}}”},
  shorttitle = {Sustainable {{Capital}}?},
