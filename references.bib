@article{mercure2021RiskopportunityAnalysisTransformative,
  title = {Risk-Opportunity Analysis for Transformative Policy Design and Appraisal},
  author = {Mercure, Jean-Francois and Sharpe, Simon and Vinuales, Jorge E. and Ives, Matthew and Grubb, Michael and Lam, Aileen and Drummond, Paul and Pollitt, Hector and Knobloch, Florian and Nijsse, Femke J. M. M.},
