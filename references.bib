@article{bastoslima2022JustTransitionBioeconomy,
  title = {Just Transition towards a Bioeconomy: {{Four}} Dimensions in {{Brazil}}, {{India}} and {{Indonesia}}},
  shorttitle = {Just Transition towards a Bioeconomy},
  author = {Bastos Lima, Mairon G.},
  date = {2022-03},
  journaltitle = {Forest Policy and Economics},
