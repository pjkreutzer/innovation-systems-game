@article{zawislak2013TypesInnovationLowTechnology,
  title = {Types of Innovation in Low-Technology Firms of Emerging Markets: An Empirical Study in Brazilian Industry},
  shorttitle = {Types of Innovation in Low-Technology Firms of Emerging Markets},
  author = {Zawislak, Paulo Antônio and Zen, Aurora Carneiro and Fracasso, Edi Madalena and Reichert, Fernanda Maciel and Pufal, Nathália Amarante},
