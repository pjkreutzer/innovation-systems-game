@article{bonaiuti2018AreWeEntering,
  title = {Are We Entering the Age of Involuntary Degrowth? {{Promethean}} Technologies and Declining Returns of Innovation},
  shorttitle = {Are We Entering the Age of Involuntary Degrowth?},
  author = {Bonaiuti, Mauro},
