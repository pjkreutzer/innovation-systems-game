  title = {The Impact of Renewable Energy on Decoupling Economic Growth from Ecological Footprint – {{An}} Empirical Analysis of 166 Countries},
  author = {Wang, Qiang and Zhang, Fuyu and Li, Rongrong and Li, Lejia},
