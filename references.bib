@article{mazzolini2018HeapsLawStatistics,
  title = {Heaps' Law, Statistics of Shared Components, and Temporal Patterns from a Sample-Space-Reducing Process},
  author = {Mazzolini, Andrea and Colliva, Alberto and Caselle, Michele and Osella, Matteo},
  date = {2018-11-27},
