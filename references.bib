@article{jung2019ImpactsAbruptLand,
  title = {Impacts of Past Abrupt Land Change on Local Biodiversity Globally},
  author = {Jung, Martin and Rowhani, Pedram and Scharlemann, Jörn P. W.},
  date = {2019-12},
  journaltitle = {Nature Communications},
