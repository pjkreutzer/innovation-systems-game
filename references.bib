@article{dedehayir2022InnovationEcosystemsStructures,
  title = {Innovation Ecosystems as Structures: {{Actor}} Roles, Timing of Their Entrance, and Interactions},
  shorttitle = {Innovation Ecosystems as Structures},
  author = {Dedehayir, Ozgur and Mäkinen, Saku J. and Ortt, J. Roland},
