@article{ibarra2022ScientistsClimateGovernance,
  title = {Scientists and Climate Governance: {{A}} View from the {{South}}},
  shorttitle = {Scientists and Climate Governance},
  author = {Ibarra, Cecilia and Jiménez, Guadalupe and O’Ryan, Raúl and Blanco, Gustavo and Cordero, Luis and Insunza, Ximena and Moraga, Pilar and Rojas, Maisa and Sapiains, Rodolfo},
