@article{asada2020BioeconomicTransitionProjecting,
  title = {Bioeconomic Transition?: {{Projecting}} Consumption-Based Biomass and Fossil Material Flows to 2050},
  shorttitle = {Bioeconomic Transition?},
  author = {Asada, Raphael and Krisztin, Tamás and family=Fulvio, given=Fulvio, prefix=di, useprefix=true and Kraxner, Florian and Stern, Tobias},
