@article{falcone2020SustainableForestbasedBioeconomy,
  title = {Towards a Sustainable Forest-Based Bioeconomy in {{Italy}}: {{Findings}} from a {{SWOT}} Analysis},
  shorttitle = {Towards a Sustainable Forest-Based Bioeconomy in {{Italy}}},
  author = {Falcone, Pasquale Marcello and Tani, Almona and Tartiu, Valentina Elena and Imbriani, Cesare},
  date = {2020-01-01},
  journaltitle = {Forest Policy and Economics},
