@article{wensing2019ProenvironmentalValuesBeliefs,
  title = {Do Pro-Environmental Values, Beliefs and Norms Drive Farmers' Interest in Novel Practices Fostering the {{Bioeconomy}}?},
  author = {Wensing, Joana and Carraresi, Laura and Bröring, Stefanie},
