  author = {Brysse, Keynyn and Oreskes, Naomi and O’Reilly, Jessica and Oppenheimer, Michael},
  date = {2013-02},
  journaltitle = {Global Environmental Change},
  shortjournal = {Global Environmental Change},
  volume = {23},
