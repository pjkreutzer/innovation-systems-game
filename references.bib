@article{reich2022EvenModestClimate,
  title = {Even Modest Climate Change May Lead to Major Transitions in Boreal Forests},
  author = {Reich, Peter B. and Bermudez, Raimundo and Montgomery, Rebecca A. and Rich, Roy L. and Rice, Karen E. and Hobbie, Sarah E. and Stefanski, Artur},
