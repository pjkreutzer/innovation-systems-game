@misc{chaminadeTransitioningPathsSustainable,
  title = {Transitioning Paths to a Sustainable Forest-Based Bioeconomy in {{Sweden}}?  {{A}} Deeper Look at the Innovation Networks},
  author = {Chaminade, Cristina and Bayuo, Blaise},
  abstract = {Scholars in the innovation system literature are increasingly paying attention to the issue of the directionality of sustainability transformations (Lindner et al. 2016; Scordato, Bugge, and Fevolden 2017; Schot and Kanger 2018) and how innovation ought to be geared towards particular visions of sustainable futures in what has been labelled as a normative turn of innovation system research (Schlaile et al. 2017). However, with few exceptions (Urmetzer et al. 2018), the literature has remained rather theoretical and its empirical applications largely unexplored. Using historical data on Swedish innovations, this paper investigates the historical transition to bioeconomy in Sweden, questioning the extent to which such transition is built around innovations in the forest-industry. The networks of innovations from SWINNO show a clear dominance of innovations coming from the machinery and equipment sector as well as chemicals but where innovations to and from the forest industry are still in an infant stage. All in all, the paper shows that Sweden is anchored in a resource-based vision of the bioeconomy, aiming at converting bioresources into new products, reducing waste, CO2 emissions and energy consumption. But has not fully embraced the potential of transformation of the forest industry to become a central piece in the path towards a stronger bioeconomy, by adopting a wider transformative strategy that embraces both the bioresource as well as bioecology vision of the forest-based bioeconomy and supports upstream innovations.},
